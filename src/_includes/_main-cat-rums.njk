---
layout: layout.njk
---
<main>
  <stack-l>
    {{ content | safe }}
    <h2>Molassses &amp; Evaporated Cane Rums</h2>
    <ul>
      {%- for post in collections.rumsAscending -%}
        <li{% if page.url == post.url %} aria-current="page"{% endif %}><a href="{{ post.url }}">{{ post.data.title }}&nbsp;<icon-l space="1em"><span class="with-icon"><svg><use href="{{ post.data.category }}"></use></svg><span class="sr-only">(1)</span></span></icon-l></a></li>
      {%- endfor -%}
    </ul>
    <h2>Fresh Cane Juice Rums</h2>
    <ul>
      {%- for post in collections.rhumsAscending -%}
        <li{% if page.url == post.url %} aria-current="page"{% endif %}><a href="{{ post.url }}">{{ post.data.title }}</a></li>
      {%- endfor -%}
    </ul>
  </stack-l>
</main>
