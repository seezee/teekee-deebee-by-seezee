---
layout: layout.njk
---
<main>
  <stack-l>
    {{ content | safe }}
    <switcher-l threshold="32.5rem">
      <stack-l>
        <h2>Molasses &amp; Evaporated Cane Rums</h2>
        <ul class="index">
          {%- for post in collections.rumsAscending -%}
            <li{% if page.url == post.url %} aria-current="page"{% endif %}><a href="{{ post.url }}">{{ post.data.title }}<icon-l space="1em" class="bigger" label="{{ post.data.label }}"><span class="with-icon"><svg class="icon"><use href="{{ post.data.category }}"></use></svg></span></icon-l></a></li>
          {%- endfor -%}
        </ul>
      </stack-l>
      <stack-l>
        <h2>Fresh Cane Juice Rums</h2>
        <ul class="index">
          {%- for post in collections.rhumsAscending -%}
            <li{% if page.url == post.url %} aria-current="page"{% endif %}><a href="{{ post.url }}">{{ post.data.title }}<icon-l space="1em" class="bigger" label="{{ post.data.label }}"><span class="with-icon"><svg class="icon"><use href="{{ post.data.category }}"></use></svg></span></icon-l></a></li>
          {%- endfor -%}
        </ul>
      </stack-l>
    </switcher-l>
  </stack-l>
</main>
